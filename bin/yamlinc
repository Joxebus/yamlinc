'use strict';

var fs = require('fs');
var args = process.argv.slice(2);
var yamlinc = '../src/yamlinc.js';
var paths = [
    '../node_modules/yamlinc/src/yamlinc.js',
    './node_modules/yamlinc/src/yamlinc.js',
];

for (var i in paths) {
    if (fs.existsSync(paths[i])) {
        yamlinc = paths[i];
    }
}

require(yamlinc).run(args);
